{
    "name": "smallblind-backend",
    "version": "1.0.0",
    "description": "Backend system for SmallBlind AI services",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build": "tsc",
        "lint": "eslint 'src/**/*.{js,ts}'",
        "format": "prettier --write 'src/**/*.{js,ts}'",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:passWithNoTests": "jest --passWithNoTests",
        "prisma:migrate": "prisma migrate dev",
        "prisma:migrate:prod": "prisma migrate deploy",
        "prisma:generate": "prisma generate",
        "prisma:studio": "prisma studio",
        "db:seed": "ts-node prisma/seed.ts",
        "db:check": "ts-node scripts/db-check.ts",
        "test:api": "ts-node scripts/test-api.ts",
        "test:prisma": "ts-node scripts/test-prisma.ts",
        "test:detection": "ts-node scripts/test-detection.ts",
        "test:description": "ts-node scripts/test-image-description.ts",
        "prepare": "npm run prisma:generate"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "keywords": [
        "ai",
        "machine-learning",
        "backend",
        "typescript"
    ],
    "author": "SmallBlind Team",
    "license": "MIT",
    "dependencies": {
        "@huggingface/transformers": "^3.5.1",
        "@prisma/client": "^6.8.0",
        "@xenova/transformers": "^2.17.2",
        "axios": "^1.10.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-validator": "^7.0.1",
        "helmet": "^7.0.0",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "node-fetch": "^3.3.2",
        "pg": "^8.11.3",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/axios": "^0.9.36",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.2.3",
        "@types/pg": "^8.11.3",
        "@types/swagger-jsdoc": "^6.0.1",
        "@types/swagger-ui-express": "^4.1.3",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "prisma": "^6.7.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
}
